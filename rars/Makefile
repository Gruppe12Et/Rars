#>+ 11
rars.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/joystick.cpp $(srcdir)/timer.cpp $(srcdir)/args.cpp $(srcdir)/race_data.cpp $(srcdir)/race_manager.cpp $(srcdir)/instant_replay.cpp $(srcdir)/deprecated.cpp $(srcdir)/trackgen.cpp $(srcdir)/track.cpp $(srcdir)/report.cpp $(srcdir)/movie.cpp $(srcdir)/misc.cpp $(srcdir)/drivers.cpp $(srcdir)/driver_old.cpp $(srcdir)/driver.cpp $(srcdir)/carz.cpp  
	@echo 'creating rars.all_cpp.cpp ...'; \
	rm -f rars.all_cpp.files rars.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> rars.all_cpp.final; \
	for file in joystick.cpp timer.cpp args.cpp race_data.cpp race_manager.cpp instant_replay.cpp deprecated.cpp trackgen.cpp track.cpp report.cpp movie.cpp misc.cpp drivers.cpp driver_old.cpp driver.cpp carz.cpp ; do \
	  echo "#include \"$$file\"" >> rars.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> rars.all_cpp.final; \
	done; \
	cat rars.all_cpp.final rars.all_cpp.files  > rars.all_cpp.cpp; \
	rm -f rars.all_cpp.final rars.all_cpp.files

#>+ 3
clean-final:
	-rm -f rars.all_cpp.cpp

